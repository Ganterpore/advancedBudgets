{
	"name": "advancedbudgets",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"lint": "eslint .",
		"db": "docker-compose -f src/devops/docker-compose.yml up -d",
		"db-close": "docker-compose -f src/devops/docker-compose.yml down",
		"migrate:make": "node-pg-migrate create -j ts",
		"migrate:up": "dotenv -e .env tsx migrate.ts up",
		"migrate:down": "dotenv -e .env tsx migrate.ts down"
	},
	"devDependencies": {
		"@iconify-json/ep": "^1.1.15",
		"@iconify-json/fa-solid": "^1.1.8",
		"@iconify-json/fluent": "^1.1.53",
		"@iconify-json/la": "^1.1.8",
		"@iconify-json/map": "^1.1.9",
		"@iconify-json/material-symbols": "^1.1.79",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "^8.56.0",
		"@types/jsonwebtoken": "^9.0.6",
		"@types/pg": "^8.11.5",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"dotenv": "^16.4.5",
		"eslint": "^8.56.0",
		"eslint-plugin-svelte": "^2.35.1",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tslib": "^2.4.1",
		"tsx": "^4.7.3",
		"typescript": "^5.0.0",
		"vite": "^5.0.3",
		"vitest": "^1.2.0"
	},
	"type": "module",
	"dependencies": {
		"@auth0/auth0-spa-js": "^2.1.3",
		"dotenv-cli": "^7.4.1",
		"jose": "^5.3.0",
		"jsonwebtoken": "^9.0.2",
		"node-pg-migrate": "^7.2.0",
		"pg": "^8.11.5",
		"ts-node": "^10.9.2",
		"unplugin-icons": "^0.19.0"
	}
}
